{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "run": "npm i && nest build && pm2 start dist/main.js --name api && pm2 logs && tail -f /dev/null",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "npm i && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:commit": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "npm run test && npm run test:cov && lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:commit",
      "git add --force"
    ]
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^2.1.1",
    "@grpc/grpc-js": "^1.5.0",
    "@grpc/proto-loader": "^0.6.9",
    "@nestjs/axios": "0.0.7",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.1.1",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/swagger": "^5.2.1",
    "@ogma/nestjs-module": "^3.1.1",
    "@ogma/platform-express": "^3.0.1",
    "@ogma/platform-grpc": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "apollo-server-express": "^3.6.1",
    "axios": "^0.26.1",
    "class-transformer": "^0.4.0",
    "class-validator": "0.13.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "gm": "^1.23.1",
    "graphql": "^15.8.0",
    "imagemagick": "^0.1.3",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.1.7",
    "minio": "^7.0.28",
    "nest-winston": "^1.6.2",
    "nestjs-minio": "^2.1.16",
    "nestjs-minio-client": "^1.2.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "prom-client": "^14.0.1",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "sharp": "^0.30.7",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/amqplib": "^0.8.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/gm": "^1.18.12",
    "@types/jest": "^27.0.2",
    "@types/minio": "^7.0.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.30.4",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
